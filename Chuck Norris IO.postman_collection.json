							"//Testing Headers\r",
							"pm.test(\"Content-Type header is present\", () => {\r",
							"  pm.response.to.have.header(\"Content-Type\");\r",
							"});\r",
							"\r",
							"pm.test(\"Content-Type header is application/json\", () => {\r",
							"  pm.expect(pm.response.headers.get('title')).to.eql('Chuck Norris Jokes Api/json');\r",
							"});\r",
							"\r",
							"pm.test('Page title should be set', function () {\r",
							"    pm.expect($('title').text()).to.equal('Chuck Norris Jokes Api'); \r",
							"});\r",
							"\r",
							"pm.test('Page should have header and footer tags', function () {\r",
							"    pm.expect($('header')).to.exist;\r",
							"    pm.expect($('footer')).to.exist;\r",
							"});\r",
							"\r",
							"pm.test(\"Content-Type is present\", function () {\r",
							"    pm.response.to.have.header.eql\r",
							"});\r",
							"\r",
							"pm.test(\"Correct title is returned\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"Chuck Norris Jokes Api\");\r",
							"});"
